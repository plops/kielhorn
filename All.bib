@article{Reddy2008,
abstract = {The dynamic ability of neuronal dendrites to shape and integrate synaptic responses is the hallmark of information processing in the brain. Effectively studying this phenomenon requires concurrent measurements at multiple sites on live neurons. Significant progress has been made by optical imaging systems which combine confocal and multiphoton microscopy with inertia-free laser scanning. However, all systems developed to date restrict fast imaging to two dimensions. This severely limits the extent to which neurons can be studied, since they represent complex three-dimensional (3D) structures. Here we present a novel imaging system that utilizes a unique arrangement of acousto-optic deflectors to steer a focused ultra-fast laser beam to arbitrary locations in 3D space without moving the objective lens. As we demonstrate, this highly versatile random-access multiphoton microscope supports functional imaging of complex 3D cellular structures such as neuronal dendrites or neural populations at acquisition rates on the order of tens of kilohertz.},
annote = {        From Duplicate 1 (                           \{T\}hree-dimensional random access multiphoton microscopy for functional imaging of neuronal activity                         - Reddy, Duemani G; Kelleher, K; Fink, R; Saggau, P )
And  Duplicate 2 (                           \{T\}hree-dimensional random access multiphoton microscopy for functional imaging of neuronal activity                         - Reddy, Duemani G; Kelleher, K; Fink, R; Saggau, P )
                
        
        
      },
author = {Reddy, Duemani G and Kelleher, K and Fink, R and Saggau, P},
file = {:home/martin/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Reddy et al. - 2008 - \{T\}hree-dimensional random access multiphoton microscopy for functional imaging of neuronal activity.pdf:pdf},
journal = {Nat. Neurosci.},
month = jun,
pages = {713--720},
title = {{Three-dimensional random access multiphoton microscopy for functional imaging of neuronal activity}},
url = {http://www.ncbi.nlm.nih.gov/pmc/articles/PMC2747788/pdf/nihms129126.pdf},
volume = {11},
year = {2008}
}
@article{Redies2007,
abstract = {When artists portray human faces, they generally endow their portraits with properties that render the faces esthetically more pleasing. To obtain insight into the changes introduced by artists, we compared Fourier power spectra in photographs of faces and in portraits by artists. Our analysis was restricted to a large set of monochrome or lightly colored portraits from various Western cultures and revealed a paradoxical result. Although face photographs are not scale-invariant, artists draw human faces with statistical properties that deviate from the face photographs and approximate the scale-invariant, fractal-like properties of complex natural scenes. This result cannot be explained by systematic differences in the complexity of patterns surrounding the faces or by reproduction artifacts. In particular, a moderate change in gamma gradation has little influence on the results. Moreover, the scale-invariant rendering of faces in artists' portraits was found to be independent of cultural variables, such as century of origin or artistic techniques. We suggest that artists have implicit knowledge of image statistics and prefer natural scene statistics (or some other rules associated with them) in their creations. Fractal-like statistics have been demonstrated previously in other forms of visual art and may be a general attribute of esthetic visual stimuli.},
author = {Redies, Christoph and H\"{a}nisch, Jan and Blickhan, Marko and Denzler, Joachim},
doi = {10.1080/09548980701574496},
file = {:home/martin/Downloads2/Redies07-Aph.pdf:pdf},
issn = {0954-898X},
journal = {Network (Bristol, England)},
keywords = {Computer-Assisted,Computer-Assisted: methods,Face,Fourier Analysis,Humans,Image Interpretation,Nature,Photic Stimulation,Portraits as Topic,Visual Perception,Visual Perception: physiology},
month = sep,
number = {3},
pages = {235--48},
pmid = {17852751},
title = {{Artists portray human faces with the Fourier statistics of complex natural scenes.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17852751},
volume = {18},
year = {2007}
}
