issn = {1365-2818},
journal = {Journal of microscopy},
keywords = {Animals,Cattle,Cell Line,Fluorescence,Fluorescent Dyes,Imaging, Three-Dimensional,Lighting,Mice,Microscopy, Fluorescence,Microscopy, Fluorescence: instrumentation,Muntjacs,Sensitivity and Specificity},
month = jan,
