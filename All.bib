file = {:home/martin/from-uffz/p1019/STORM\_3D.pdf:pdf;:home/martin/Downloads2/Huang.SOM.pdf:pdf},
issn = {1095-9203},
journal = {Science (New York, N.Y.)},
keywords = {Animals,Carbocyanines,Cell Line,Cell-Membrane,Cell-Membrane: ultrastructure,Cercopithecus aethiops,Clathrin,Coated Pits,Computer-Assisted,Cyclic AMP,Cyclic AMP: analogs \& derivatives,Fluorescence,Fluorescence: instrumentation,Fluorescence: methods,Fluorescent Antibody Technique,Fluorescent Dyes,Image Processing,Imaging,Microscopy,Microtubules,Microtubules: ultrastructure,Nanotechnology,Quantum Dots,Stochastic Processes,Streptavidin,Three-Dimensional,Three-Dimensional: instrumentation,Three-Dimensional: methods},
month = feb,
