file = {:home/martin/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Otsu et al. - 2008 - \{O\}ptical monitoring of neuronal activity at high frame rate with a digital random-access multiphoton (\{R\}\{A\}\{M\}\{P\}) microscope.pdf:pdf},
journal = {J. Neurosci. Methods},
month = aug,
pages = {259--270},
title = {{\{O\}ptical monitoring of neuronal activity at high frame rate with a digital random-access multiphoton (\{R\}\{A\}\{M\}\{P\}) microscope}},
url = {http://cnp.igc.gulbenkian.pt/\_media/research/mainen/ghyomm/otsu\_2008.pdf},
volume = {173},
